//Min and Max in an array
#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;i<n-1;j++)
        {
            if(a[j]>a[j+1])
            {
                int temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
    printf("%d %d",a[0],a[n-1]);
}
//swap program using pointers
#include <stdio.h>
int swap(int *a,int *b)
{
    int temp=*a;
    *a=*b;
    *b=temp;
    return *a,*b;
}
int main() {
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d %d",a,b);
    swap(&a,&b);
printf("\n%d %d",a,b);
   
}
